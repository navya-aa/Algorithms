def insertionsort(list):
    for i in range(1,len(list)):
        value = list[i]
        for j in range(i-1,0,-1):
            if(list[j] > value):
                list[j+1] = list[j]
            else:
                list[j+1] = value
                break
            
list =[1,4,5,3,2,8,11,20]
insertionsort(list)
print(list)
